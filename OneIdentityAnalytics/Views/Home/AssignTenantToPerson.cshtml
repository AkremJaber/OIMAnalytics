@model OneIdentityAnalytics.Controllers.HomeController.AssigningTenantToPerson

@{ var tenantOptions = Model.TenantOptions;
    var person = Model.person;
   // var tenant = Model.;
    }

<h2>Assign new tenant to @person.CentralAccount</h2>
<form class="form-horizontal" method="post">
    <div class="form-group row">
        <label for="TenantName" class="col-sm-3 col-form-label">Tenants</label>
        <div class="col-sm-9">
            <select class="form-control" id="TenantName" name="TenantName" asp-items="tenantOptions">
                <option selected="selected" value=></option>
            </select>
        </div>
    </div>
    <div class="form-group row button-row">
        <button id="save-user-button" type="submit" class="btn btn-primary">
            <i class="fa fa-user-plus"></i>
            Assign
        </button>
    </div>
</form>

